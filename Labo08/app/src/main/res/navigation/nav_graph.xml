<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/billboardFragment">

    <fragment
        android:id="@+id/billboardFragment"
        android:name="com.example.labo08.ui.movie.billboardFragment"
        android:label="billboardFragment" >
        <action
            android:id="@+id/action_billboardFragment_to_movieFragment"
            app:destination="@id/movieFragment" />
        <action
            android:id="@+id/action_billboardFragment_to_newMovieFragment"
            app:destination="@id/newMovieFragment" />
    </fragment>
    <fragment
        android:id="@+id/newMovieFragment"
        android:name="com.example.labo08.ui.movie.newmovie.newMovieFragment"
        android:label="newMovieFragment" />
    <fragment
        android:id="@+id/movieFragment"
        android:name="com.example.labo08.ui.movie.movieFragment"
        android:label="movieFragment" />
</navigation>